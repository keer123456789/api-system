<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.keer.underline.mapper.TaskMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.keer.underline.entity.Task">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="customer" property="customer" />
        <result column="sale_man" property="saleMan" />
        <result column="api_id" property="apiId" />
        <result column="param" property="param" />
        <result column="result_path" property="resultPath" />
        <result column="run_num" property="runNum" />
        <result column="download_num" property="downloadNum" />
        <result column="status" property="status" />
        <result column="test_file_path" property="testFilePath" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, customer, sale_man, api_id, param, result_path, run_num, download_num, status, test_file_path, create_time, update_time
    </sql>

</mapper>
